{
  "$schema": "https://json.schemastore.org/package",
  "author": "Romain Racamier-Lafon <romain.racamier@gmail.com>",
  "bugs": "https://github.com/Shuunen/stuff-finder/issues",
  "dependencies": {
    "@camwiegert/typical": "0.1",
    "@emotion/react": "11.13",
    "@emotion/styled": "11.13",
    "@mui/icons-material": "5.16",
    "@mui/material": "5.16",
    "@preact/signals": "1.3",
    "@zxing/library": "0.21",
    "fuse.js": "7.0",
    "notistack": "3.0",
    "preact": "10.23",
    "preact-router": "4.1",
    "shuutils": "8.1",
    "valibot": "0.36",
    "webcomponent-qr-code": "1.2"
  },
  "description": "Sorting things is pointless if you can't find them afterwards",
  "devDependencies": {
    "@babel/plugin-syntax-class-properties": "*",
    "@babel/plugin-syntax-decorators": "*",
    "@babel/plugin-syntax-jsx": "*",
    "@biomejs/biome": "1.8",
    "@preact/preset-vite": "2.9",
    "@stylistic/eslint-plugin": "2.6",
    "@types/dom-speech-recognition": "0",
    "@types/node": "20",
    "@vitest/coverage-v8": "2.0",
    "autoprefixer": "10",
    "dependency-cruiser": "latest",
    "eslint": "*",
    "eslint-plugin-shuunen": "0.3",
    "postcss": "8.4",
    "repo-check": "1.37",
    "tailwindcss": "3.4",
    "typescript": "5.5",
    "vite": "5.3",
    "vitest": "2.0"
  },
  "files": [
    "src"
  ],
  "homepage": "https://github.com/Shuunen/stuff-finder",
  "keywords": [
    "webapp",
    "stuff",
    "finder"
  ],
  "license": "GPL-3.0",
  "name": "@shuunen/stuff-finder",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Shuunen/stuff-finder.git"
  },
  "scripts": {
    "build": "tsc && echo tsc success && vite build && echo vite build success && pnpm mark && echo build success",
    "check": "pnpm install && repo-check && pnpm build && pnpm lint && pnpm test && echo check success",
    "dev": "vite",
    "lint": "pnpm lint:tsc && pnpm lint:biome && pnpm lint:eslint && pnpm lint:dep && echo lint success",
    "lint:biome": "biome lint --error-on-warnings --write --unsafe src && echo lint:biome success",
    "lint:dep": "depcruise src --config && echo lint:dep success",
    "lint:eslint": "eslint --cache --max-warnings 0 src && echo lint:eslint success",
    "lint:tsc": "tsc --noEmit && echo lint:tsc success",
    "mark": "unique-mark dist/index.html && echo mark success",
    "postversion": "git push && git push --tags",
    "preversion": "pnpm check",
    "serve": "pnpm build && vite preview",
    "test": "vitest --coverage --run && echo test success",
    "test:bench": "vitest bench",
    "test:update": "vitest --run --update && echo test update success",
    "test:watch": "vitest --coverage"
  },
  "type": "module",
  "version": "2.0.0"
}