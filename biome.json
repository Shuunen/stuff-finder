{
  "$schema": "https://biomejs.dev/schemas/1.7.3/schema.json",
  "formatter": {
    "enabled": true
  },
  "linter": {
    "enabled": true,
    "rules": {
      "a11y": {
        "useMediaCaption": "off"
      },
      "all": true,
      "complexity": {
        "noVoid": "off",
        "useLiteralKeys": "off"
      },
      "style": {
        "noImplicitBoolean": "off",
        "useBlockStatements": "off"
      }
    }
  },
  "organizeImports": {
    "enabled": true
  }
}